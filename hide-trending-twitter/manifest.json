{
    "manifest_version": 2,
    "name": "Trending Hashtag Hider",
    "version": "0.1",
  
    "description": "Hide the annoying and unnecessary trending hashtag on twitter",
  
    "icons": {
      "48": "icons/48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.twitter.com/*"],
        "js": ["hide.js"]
      }
    ]
  
  }
